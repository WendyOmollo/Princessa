{{#partial "content"}}

    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <div class="row">
                    <h1>Princesses</h1>
                </div>
                <div class="row">
                {{#if princesses }}
                {{#each princesses}}
                <div class="col-md-4">
                    <div class="card">
                        <div class="card-body">
                            <h4 class="card-title"><strong>{{ name }}</strong></h4>
                            <a class="btn btn secondary" href="/princesses/{{id}}">Info</a>
                        </div>
                    </div>
                </div>
                {{/each}}
                {{ else }}
                <p>Waiting...</p>
                    {{/if}}
                </div>
            </div>
            <div class="col-md-4">
                <div class="row">
                    <h1>Domains</h1>
                </div>
                <div class="row">
                    {{#if domains}}
                        {{#each domains}}
                            <div class="col-md-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title"><strong>{{ name }}</strong></h4>
                                        <a class="btn btn-secondary" href="/domains/{{id}}">Info</a>
                                    </div>
                                </div>
                            </div>
                        {{/each}}
                    {{ else }}
                        <p>Waiting...</p>
                    {{/if}}
                </div>
            </div>
        </div>
    </div>

{{/partial}}


{{> layout.hbs}}